{
    "name": "bussiness/proverb",
    "description": "lumenless",
    "keywords": ["framework", "laravel", "lumen", "lumenless"],
    "license": "private",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.2345.com/repo/private/"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "ext-amqp": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/mail": "^7.10",
        "illuminate/redis": "^7.6",
        "laravel/lumen-framework": "^7.0",
        "laravelista/lumen-vendor-publish": "^7.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "domains\\": "domains/",
            "infra\\": "infra/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "process-timeout": 1800,
        "secure-http" :false
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
